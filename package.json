{
  "name": "git-stack-cli",
  "version": "1.0.8",
  "description": "",
  "author": "magus",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/magus/git-stack-cli.git"
  },
  "type": "module",
  "bin": {
    "git-stack": "dist/cjs/index.cjs"
  },
  "files": [
    "dist/cjs",
    "scripts",
    "src",
    "package-lock.json",
    "rollup.config.mjs",
    "tsconfig.json"
  ],
  "scripts": {
    "dev": "npm run build -- --watch",
    "build": "rollup -c rollup.config.mjs",
    "build:standalone": "bun run scripts/build-standalone.ts",
    "release:npm": "bun run scripts/release-npm.ts",
    "release:github": "bun run scripts/release-github.ts",
    "release:brew": "bun run scripts/release-brew.ts",
    "release": "npm run release:npm && npm run release:github && npm run release:brew",
    "lint:check": "eslint . --cache",
    "lint": "npm run lint:check -- --fix",
    "prettier:check": "prettier ./src --check --cache",
    "prettier": "npm run prettier:check -- --write",
    "test": "bun test",
    "test:watch": "npm run test -- --watch",
    "test:types": "tsc",
    "test:all": "npm run prettier:check && npm run lint:check && npm run test:types",
    "prepublishOnly": "bun run scripts/npm-prepublishOnly.ts"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "immer": "^10.0.3",
    "ink-cjs": "4.4.1",
    "luxon": "^3.4.4",
    "react": "^18.2.0",
    "react-devtools-core": "^4.19.1",
    "react-intl": "^6.5.5",
    "yargs": "^17.7.2",
    "zustand": "^4.4.4"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/chalk": "^2.2.0",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.8.7",
    "@types/react": "^18.2.33",
    "@types/yargs": "^17.0.29",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "bun": "^1.0.26",
    "bun-types": "^1.0.21",
    "eslint": "^8.52.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-react": "^7.33.2",
    "pkg": "^5.8.1",
    "prettier": "^3.0.3",
    "rollup": "^4.10.0",
    "typescript": "^5.2.2"
  }
}
